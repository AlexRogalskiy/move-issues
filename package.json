{
  "name": "move-issues",
  "version": "0.1.0",
  "description": "A GitHub App built with Probot that moves issues.",
  "author": "Armin Sebastian",
  "license": "MIT",
  "repository": "https://github.com/dessant/move-issues",
  "scripts": {
    "start": "probot run ./src/index.js",
    "watch": "nodemon --exec 'npm start'",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version && yarn run push"
  },
  "dependencies": {
    "moment": "^2.19.2",
    "probot": "^3.0.2",
    "probot-commands": "1.0.0",
    "to-markdown": "^3.1.0"
  },
  "devDependencies": {
    "localtunnel": "^1.8.3",
    "nodemon": "1.12.1",
    "npm-check-updates": "^2.13.0",
    "standard-version": "^4.2.0"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  }
}
